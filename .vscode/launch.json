    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "auto",
            "type": "auto-debug",
            "request": "launch",
            "map": {
                "*.py": "Python: Current File"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\export.js"
        }
    ]
}